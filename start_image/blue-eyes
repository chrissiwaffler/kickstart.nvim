\e[49m                  \e[38;5;152;48;5;254mâ–„\e[38;5;237;48;5;152mâ–„\e[49;38;5;237mâ–€\e[38;5;255;48;5;240mâ–„\e[38;5;254;48;5;7mâ–„\e[49;38;5;66mâ–€\e[49;38;5;247mâ–€\e[38;5;249;48;5;109mâ–„\e[38;5;59;48;5;73mâ–„\e[49m        \e[49;38;5;232mâ–€\e[49;38;5;251mâ–€\e[38;5;236;48;5;0mâ–„\e[49;38;5;8mâ–€\e[49m                \e[m
\e[49m                  \e[49;38;5;8mâ–€\e[49m  \e[49;38;5;240mâ–€\e[49;38;5;239mâ–€\e[49m  \e[49;38;5;234mâ–€\e[49m                             \e[m
"
center_text() {
    text="$1"
    width=$(tput cols)
    
    # Remove ANSI escape codes for length calculation
    text_length=$(echo -e "$text" | sed 's/\x1b\[[0-9;]*m//g' | wc -m)
    
    padding=$(( (width - text_length) / 2 ))
    printf "%${padding}s%b\n" "" "$text"
}
# center_text "\e[33;1mHEART OF THE CARDS, GUIDE ME!\e[0m"
center_text "\e[36;1mH E A R T  O F  T H E  C A R D S ,  G U I D E  M E !\e[0m"
